{
  "name": "@admin-core/form-react",
  "version": "0.0.1",
  "description": "React 表单适配包，仅负责 UI 渲染与组件映射",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/form-react.css",
    "./style.css": "./dist/form-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest --passWithNoTests",
    "test:run": "vitest run --passWithNoTests",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/form-core": "workspace:*"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-test-renderer": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "admin",
    "form",
    "react",
    "adapter"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/form/react"
  },
  "publishConfig": {
    "access": "public"
  }
}
