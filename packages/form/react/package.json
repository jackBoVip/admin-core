{
  "name": "@admin-core/form-react",
  "version": "1.0.0",
  "description": "React 表单适配包，仅负责 UI 渲染与组件映射",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/form-react.css",
    "./style.css": "./dist/form-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vite build --config vite.cdn.config.ts --mode production && vite build --config vite.cdn.config.ts --mode development",
    "dev": "vite build --watch",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/react-disable-import-order.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/form-core": "workspace:*",
    "@admin-core/form-shared": "workspace:*",
    "@admin-core/shared-react": "workspace:*"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-test-renderer": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "admin",
    "form",
    "react",
    "adapter"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/form/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundledDependencies": [
    "@admin-core/form-core",
    "@admin-core/form-shared",
    "@admin-core/shared-react"
  ]
}
