{
  "name": "@admin-core/layout-react",
  "version": "1.0.2",
  "description": "React 基础布局组件，开箱即用，高度集成偏好设置",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/layout-react.css",
    "./style.css": "./dist/layout-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vite build --config vite.cdn.config.ts --mode production && vite build --config vite.cdn.config.ts --mode development",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/layout-react.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/layout": "workspace:*",
    "@admin-core/layout-shared": "workspace:*",
    "@admin-core/preferences": "workspace:*",
    "@admin-core/preferences-react": "workspace:*",
    "@admin-core/shared-react": "workspace:*",
    "@floating-ui/react": "^0.27.17"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@iconify-json/lucide": "catalog:",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@types/react-test-renderer": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-test-renderer": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-router-dom": ">=6.0.0",
    "tailwindcss": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "tailwindcss": {
      "optional": true
    },
    "react-router-dom": {
      "optional": true
    }
  },
  "keywords": [
    "admin",
    "layout",
    "react",
    "sidebar",
    "header",
    "tabbar",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/layout/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundledDependencies": [
    "@admin-core/layout",
    "@admin-core/layout-shared",
    "@admin-core/preferences",
    "@admin-core/shared-react"
  ]
}
