{
  "name": "@admin-core/layout",
  "version": "1.0.2",
  "description": "基础布局包核心，提供类型定义、配置、国际化和工具函数",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./locales/*": {
      "import": "./dist/locales/*.js",
      "require": "./dist/locales/*.cjs"
    },
    "./styles": {
      "import": "./dist/styles/index.js",
      "require": "./dist/styles/index.cjs"
    },
    "./styles/layout.css": "./dist/styles/layout.css"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/layout-core.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/layout-core.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/layout-core.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {},
  "dependencies": {
    "@admin-core/shared-core": "workspace:*",
    "@admin-core/preferences": "workspace:*"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "admin",
    "layout",
    "sidebar",
    "header",
    "tabbar",
    "i18n"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/layout/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": true
}
