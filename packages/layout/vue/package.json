{
  "name": "@admin-core/layout-vue",
  "version": "0.0.1",
  "description": "Vue 3 基础布局组件，开箱即用，高度集成偏好设置",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/layout-vue.css",
    "./style.css": "./dist/layout-vue.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest --passWithNoTests",
    "test:run": "vitest run --passWithNoTests",
    "lint": "eslint src",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/layout": "workspace:*",
    "@admin-core/preferences": "workspace:*",
    "@admin-core/preferences-vue": "workspace:*",
    "@floating-ui/vue": "^1.1.10"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@iconify-json/lucide": "catalog:",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vueuse/core": "catalog:",
    "eslint": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vue-tsc": "catalog:"
  },
  "peerDependencies": {
    "tailwindcss": ">=4.0.0",
    "vue": ">=3.3.0",
    "vue-router": ">=4.0.0"
  },
  "keywords": [
    "admin",
    "layout",
    "vue",
    "vue3",
    "sidebar",
    "header",
    "tabbar",
    "tailwindcss"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/layout/vue"
  },
  "publishConfig": {
    "access": "public"
  }
}
