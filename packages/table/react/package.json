{
  "name": "@admin-core/table-react",
  "version": "1.0.0",
  "description": "React 表格适配包，基于 antd Table 并内置搜索表单集成",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/table-react.css",
    "./style.css": "./dist/table-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run typecheck && vite build && vite build --config vite.cdn.config.ts --mode production && vite build --config vite.cdn.config.ts --mode development",
    "dev": "vite build --watch",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/react-disable-import-order.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/form-react": "workspace:*",
    "@admin-core/preferences": "workspace:*",
    "@admin-core/shared-react": "workspace:*",
    "@admin-core/table-core": "workspace:*",
    "@admin-core/table-shared": "workspace:*"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-test-renderer": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "antd": ">=5.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "keywords": [
    "admin",
    "table",
    "react",
    "antd",
    "grid"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/table/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundledDependencies": [
    "@admin-core/preferences",
    "@admin-core/shared-react",
    "@admin-core/table-core",
    "@admin-core/table-shared"
  ]
}
