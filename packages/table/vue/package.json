{
  "name": "@admin-core/table-vue",
  "version": "1.0.2",
  "description": "Vue 3 表格适配包，基于 vxe-table 并内置搜索表单集成",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": "./dist/table-vue.css",
    "./style.css": "./dist/table-vue.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run typecheck && vite build && vite build --config vite.cdn.config.ts --mode production && vite build --config vite.cdn.config.ts --mode development",
    "dev": "vite build --watch",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/vue-disable-import-order.config.js src --max-warnings=0",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/form-vue": "workspace:*",
    "@admin-core/preferences": "workspace:*",
    "@admin-core/shared-vue": "workspace:*",
    "@admin-core/table-core": "workspace:*",
    "@admin-core/table-shared": "workspace:*"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/test-utils": "catalog:",
    "eslint": "catalog:",
    "jsdom": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "peerDependencies": {
    "vue": ">=3.3.0",
    "vxe-pc-ui": ">=4.0.0",
    "vxe-table": ">=4.0.0"
  },
  "keywords": [
    "admin",
    "table",
    "vue",
    "vxe",
    "grid"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/table/vue"
  },
  "publishConfig": {
    "access": "public"
  },
  "bundledDependencies": [
    "@admin-core/preferences",
    "@admin-core/shared-vue",
    "@admin-core/table-core",
    "@admin-core/table-shared"
  ]
}
