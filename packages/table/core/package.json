{
  "name": "@admin-core/table-core",
  "version": "1.0.1",
  "description": "框架无关的表格核心引擎，提供统一 Table API、代理请求增强与渲染器协议",
  "type": "module",
  "sideEffects": [
    "*.css"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.global.js",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./styles": {
      "import": "./dist/styles/index.js",
      "require": "./dist/styles/index.cjs"
    },
    "./styles/table.css": "./dist/styles/table.css",
    "./locales/*": {
      "import": "./dist/locales/*.js",
      "require": "./dist/locales/*.cjs"
    }
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "build": "pnpm run typecheck && tsup",
    "dev": "tsup --watch",
    "bench:column-custom": "pnpm run build && node ./scripts/benchmark-column-custom.mjs",
    "bench:column-custom:check": "pnpm run build && node ./scripts/benchmark-column-custom.mjs --check",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/core.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/core.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/core-disable-import-order.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@admin-core/shared-core": "workspace:*"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "admin",
    "table",
    "grid",
    "core",
    "adapter"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/table/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": true
}
