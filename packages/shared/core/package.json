{
  "name": "@admin-core/shared-core",
  "version": "1.0.1",
  "description": "Shared core primitives for @admin-core packages",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/core.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/core.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/core.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "keywords": [
    "admin",
    "shared",
    "core",
    "store"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/shared/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": true
}
