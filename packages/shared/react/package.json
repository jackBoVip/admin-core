{
  "name": "@admin-core/shared-react",
  "version": "1.0.2",
  "description": "Shared React hooks for @admin-core packages",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts",
    "test:run": "vitest -c ../../../internal/build-config/configs/vitest/dom.config.ts run",
    "lint": "eslint -c ../../../internal/build-config/configs/eslint/react-disable-import-order.config.js src --max-warnings=0",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@admin-core/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "eslint": "catalog:",
    "react": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "keywords": [
    "admin",
    "shared",
    "react",
    "hooks"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "",
    "directory": "packages/shared/react"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": true
}
